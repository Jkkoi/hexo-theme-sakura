<div id="mo-nav" class="">
  <div class="m-avatar">
    <img src="<%- (site.data.sakura.cdn || '') + site.data.sakura.avatar%>">
  </div>
  <p style="text-align: center; color: #333; font-weight: 900; font-family: 'Ubuntu', sans-serif; letter-spacing: 1.5px"><%- site.data.sakura.prefixName%><%- site.data.sakura.siteName%></p>
  <p style="text-align: center; word-spacing: 20px;">
    <% if (site.data.sakura.msocial) {%>
      <% for (var i in site.data.sakura.msocial) {%>
        <a href="<%- site.data.sakura.msocial[i].url%>" class="fa <%- site.data.sakura.msocial[i].fa%>" target="_blank" style="color: #<%- site.data.sakura.msocial[i].color%>; margin-left:20px"></a>
      <% } %>
    <% } %>
  </p>
  <ul id="menu-new-1" class="menu">
    <% for (var menu in site.data.sakura.menus){ %>
      <li>
        <a href="<%- url_for(site.data.sakura.menus[menu].path) %>">
          <span class="faa-parent animated-hover">
            <i class="fa  <%= site.data.sakura.menus[menu].fa %>" aria-hidden="true"></i>
            <%= menu %>
          </span>
        </a>
        <% if (site.data.sakura.menus[menu].submenus) { %>
          <ul class="sub-menu">
            <% for (var submenu in site.data.sakura.menus[menu].submenus){ %>
              <li>
                <a href="<%- url_for(site.data.sakura.menus[menu].submenus[submenu].path) %>">
                  <i class="fa <%= site.data.sakura.menus[menu].submenus[submenu].fa %>" aria-hidden="true"></i>
                  <%= submenu %>
                </a>
              </li>
            <% } %>
          </ul>
        <% } %>
      </li>
    <% } %>
  </ul>
  <p style="text-align: center; font-size: 13px; color: #b9b9b9;">&copy 2019 hexo-sakura</p>
</div>
<button onclick="topFunction()" class="mobile-cd-top" id="moblieGoTop" title="Go to top" style="display: none;"><i class="fa fa-chevron-up" aria-hidden="true"></i></button>