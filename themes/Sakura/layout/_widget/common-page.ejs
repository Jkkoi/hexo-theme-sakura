  <div class="pattern-center-blank"></div>
  <% if (post.photos && post.photos.length){ %>
    <div class="pattern-center ">
      <div class="pattern-attachment-img">
        <img src="<%- site.data.sakura.lazyloadImg%>" data-src="<%= post.photos %>" class="lazyload" onerror="imgError(this,3)" style="width: 100%; height: 100%; object-fit: cover; pointer-events: none;">
      </div>
      <header class="pattern-header ">
        <h1 class="entry-title">
        <%= post.keywords %></h1>
      </header>
    </div>
  <% } %>
  <div id="content" class="site-content">
    <!-- #post-## -->
    <!-- 个人信息在下面什么鬼呀！！！！ -->
    <section class="author-profile">
      <div class="info" itemprop="author" itemscope="" itemtype="https://schema.org/Person">
        <a href="/about/" class="profile gravatar"><img src="<%- (site.data.sakura.cdn || '') + site.data.sakura.avatar%>" itemprop="image" alt="<%- site.data.sakura.siteName %>" height="70" width="70"></a>
        <!-- 在我网站上丑的要死，要打开把下面注释取消就行 -->
        <!-- <div class="meta">
          <span class="title">Author</span>
          <h3 itemprop="name">
          <a href="<%- site.data.sakura.url%>" itemprop="url" rel="author"><%- site.data.sakura.siteName %></a>
          </h3>
        </div>
      </div> -->
      <hr>
      <p><i class="iconfont icon-write"></i><%- site.data.sakura.description%></p>
    </section>
    <article class="post-item post-301 page type-page status-publish has-post-thumbnail hentry">
      <%- post.content %>
      <!-- .entry-content -->
      <%- partial('_partial/comment') %>
      <% if(site.data.sakura.donate.enable){%>
      <div class="single-reward">
        <div class="reward-open">赏
          <div class="reward-main">
            <ul class="reward-row">
              <li class="alipay-code"><img src="<%- (site.data.sakura.cdn || '') + site.data.sakura.donate.alipay%>"></li>
              <li class="wechat-code"><img src="<%- (site.data.sakura.cdn || '') + site.data.sakura.donate.wechat%>"></li>
            </ul>
          </div>
        </div>
      </div>
      <%}else{%><p></p><%}%>
      <div style="text-align:center; width: 100%" class="social-share share-mobile" data-disabled="diandian, tencent"></div>
      <footer class="post-footer">
        <div class="post-lincenses"><a href="https://creativecommons.org/licenses/by-nc-sa/4.0/deed.zh" target="_blank" rel="nofollow"><i class="fa fa-creative-commons" aria-hidden="true"></i> 知识共享署名-非商业性使用-相同方式共享 4.0 国际许可协议</a></div>
        <div class="post-tags">
        </div>
        <div class="post-share">
          <div class="social-share sharehidden share-component"></div>
          <i class="iconfont show-share icon-forward"></i>
        </div>
      </footer><!-- .entry-footer -->
    </article>
  </div><!-- #primary -->
</div>